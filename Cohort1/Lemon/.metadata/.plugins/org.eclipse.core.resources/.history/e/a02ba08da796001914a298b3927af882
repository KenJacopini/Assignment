package chapter6.exercises;

public class CommonDivisor {
	
	public static void checkDivisor(int x, int y) {
		
		int m; 
		
		for(int i=1; i <x || i<y; i++) {
			if(x%i ==0 && y%i ==0) {
				int store = i;
				
				System.out.println(m = Math.max(store,i));
			}
		}System.out.println();
		
//		for(int j =1; j<y; j++) {
//			if(y%j ==0) {
//				System.out.print(j);
//			}
//		}
	}

	public static void main(String[] args) {
		checkDivisor(8,12);
	}
}
